<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Alfonso Vargas</Authors>
    <Company>Alfonso Vargas</Company>
    <Description>Integration support for Ninject in to NRules.</Description>
    <PackageTags>NRules Ninject Integration</PackageTags>
    <PackageLicenseUrl>https://github.com/alfonsovgs/NRules.Integration.Ninject/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/alfonsovgs/NRules.Integration.Ninject</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alfonsovgs/NRules.Integration.Ninject</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ninject" Version="3.3.4" />
    <PackageReference Include="NRules" Version="0.8.4" />
  </ItemGroup>

</Project>
